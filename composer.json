{
  "name": "incapption/distributed-cronjob",
  "description": "Distributes cron jobs among identical webservers based on a single remote mysql database.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "incapption",
      "email": "tastenhelden@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/incapption/DistributedCronjob"
    }
  ],
  "require": {
    "php": ">=7.2",
    "myclabs/php-enum": "^1.8",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*"
  },
  "autoload": {
    "psr-4": {
      "Incapption\\DistributedCronjob\\": "src/",
      "Incapption\\DistributedCronjob\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}